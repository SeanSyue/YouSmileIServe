{
    "manifest": {
        "displayName": "Robot goal",
        "invocationName": "Robot goal",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "com.acme.actions.robot_goal",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.acme.intents.robot_goal",
                "parameters": [
                    {
                        "name": "goal",
                        "type": "SchemaOrg_Text"
                    }
                ],
                "trigger": {
                    "queryPatterns": [
                        "I want to go to $SchemaOrg_Text:goal"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Sure, Let's go to $goal now"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.acme.commands.robot_goal",
                                    "params": {
                                        "goals": "$goal"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        }
    ]
}