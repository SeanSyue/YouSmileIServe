                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.smile.intents.NeedRecommendation",
                "trigger": {
                    "queryPatterns": [ 
                        "I don't know", 
                        "I'm not sure",
                        "I need (some)? recommendation"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Would you like noodle or rice?"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.smile.commands.NeedRecommendation"
                                }
                            }
                        ]
                    }
                }
            }
        }, 
